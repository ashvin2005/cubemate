{
  "name": "cubemate",
  "version": "1.0.0",
  "description": "An intelligent 3D Rubik's Cube solving companion",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd frontend && npm start",
    "dev:backend": "cd backend && npm run dev",
    "build": "cd frontend && npm run build",
    "test": "cd frontend && npm test && cd ../backend && npm test",
    "install:all": "npm install && cd frontend && npm install && cd ../backend && npm install",
    "clean": "rm -rf frontend/node_modules backend/node_modules node_modules"
  },
  "keywords": [
    "rubiks-cube",
    "3d-visualization",
    "education",
    "three.js",
    "react",
    "cube-solver"
  ],
  "author": "CubeMate Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-team/cubemate.git"
  }
}
